<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.diagnostics>
    <sharedListeners>
      <add  name="ServiceModelMessageLoggingListener"
      initializeData="\\minorbldsrv\_BuildDrops\Logs\Leet\Acceptatie_PcSWinkelen_web_messages.svclog"
      xdt:Locator="Match(name)"
      xdt:Transform="SetAttributes"></add>
      <add  name="ServiceModelTraceListener"
      initializeData="\\minorbldsrv\_BuildDrops\Logs\Leet\Acceptatie_PcSWinkelen_web_tracelog.svclog"
      xdt:Locator="Match(name)"
      xdt:Transform="SetAttributes"></add>
    </sharedListeners>
  </system.diagnostics>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  <serviceBus>
    <serviceLocators active="webServiceLocator" profile="Acceptatie" xdt:Transform="SetAttributes(profile)">
      <fileServiceLocator filePath="~/LocationData.xml" />
      <webServiceLocator address="http://minorbldsrv/leet/PfSLocatorService/PfSLocatorService.svc" xdt:Transform="SetAttributes(address)" binding="basicHttpBinding" />
    </serviceLocators>
  </serviceBus>
</configuration>