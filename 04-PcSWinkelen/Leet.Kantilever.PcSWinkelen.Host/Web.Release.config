<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="Configuration" value="Release" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <system.diagnostics>
    <sharedListeners>
      <add  name="ServiceModelMessageLoggingListener"
      initializeData="\\minorbldsrv\_BuildDrops\Logs\Leet\Release_PcSWinkelen_web_messages.svclog"
      xdt:Locator="Match(name)"
      xdt:Transform="SetAttributes"></add>
      <add  name="ServiceModelTraceListener"
      initializeData="\\minorbldsrv\_BuildDrops\Logs\Leet\Release_PcSWinkelen_web_tracelog.svclog"
      xdt:Locator="Match(name)"
      xdt:Transform="SetAttributes"></add>
    </sharedListeners>
  </system.diagnostics>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  <serviceBus>
    <serviceLocators active="webServiceLocator" profile="Release" xdt:Transform="SetAttributes(profile)">
      <fileServiceLocator filePath="~/LocationData.xml" />
      <webServiceLocator address="http://minorbldsrv/leet/PfSLocatorService/PfSLocatorService.svc" xdt:Transform="SetAttributes(address)" binding="basicHttpBinding" />
    </serviceLocators>
  </serviceBus>
  <connectionStrings>
    <add name="WinkelenContext" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Leet_Release_PcSWinkelenDatabase;Integrated Security=true" providerName="System.Data.SqlClient" xdt:Locator="Match(name)" xdt:Transform="Replace"/>
  </connectionStrings>
</configuration>